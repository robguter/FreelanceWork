<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_marginBottom="3dp"
    android:paddingStart="15dp"
    android:paddingTop="5dp"
    android:paddingEnd="15dp"
    android:paddingBottom="5dp"
    app:cardCornerRadius="4dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/tvwCaliid"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="2dp"
            android:layout_marginEnd="10dp"
            android:maxWidth="50dp"
            android:minWidth="45dp"
            android:textSize="14sp"
            android:paddingStart="3dp"
            android:paddingEnd="2dp"
            app:layout_constraintEnd_toStartOf="@+id/tvwTrabid"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="ID" />

        <TextView
            android:id="@+id/tvwTrabid"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:layout_marginTop="2dp"
            android:layout_marginEnd="10dp"
            android:minWidth="100dp"
            android:textSize="14sp"
            android:paddingStart="3dp"
            android:paddingEnd="2dp"
            app:layout_constraintEnd_toStartOf="@+id/tvwCalificadorid"
            app:layout_constraintStart_toEndOf="@+id/tvwCaliid"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="Trabid" />

        <TextView
            android:id="@+id/tvwCalificadorid"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:layout_marginTop="2dp"
            android:layout_marginEnd="10dp"
            android:minWidth="100dp"
            android:textSize="14sp"
            android:paddingStart="3dp"
            android:paddingEnd="2dp"
            app:layout_constraintEnd_toStartOf="@+id/tvwCalificadoid"
            app:layout_constraintStart_toEndOf="@+id/tvwTrabid"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="Calificadorid" />

        <TextView
            android:id="@+id/tvwCalificadoid"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:layout_marginTop="2dp"
            android:layout_marginEnd="10dp"
            android:minWidth="100dp"
            android:textSize="14sp"
            android:paddingStart="3dp"
            android:paddingEnd="2dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/tvwCalificadorid"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="Calificadoid" />

        <TextView
            android:id="@+id/tvwPuntuacion"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="1dp"
            android:layout_marginEnd="10dp"
            android:minWidth="100dp"
            android:textSize="14sp"
            android:paddingStart="3dp"
            android:paddingEnd="2dp"
            app:layout_constraintEnd_toStartOf="@+id/tvwComentario"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvwCaliid"
            tools:text="Puntuacion" />

        <TextView
            android:id="@+id/tvwComentario"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:layout_marginTop="1dp"
            android:layout_marginEnd="10dp"
            android:textSize="14sp"
            android:paddingStart="3dp"
            android:paddingEnd="2dp"
            app:layout_constraintEnd_toStartOf="@+id/tvwFechacali"
            app:layout_constraintStart_toEndOf="@+id/tvwPuntuacion"
            app:layout_constraintTop_toBottomOf="@+id/tvwTrabid"
            tools:text="Comentario" />

        <TextView
            android:id="@+id/tvwFechacali"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:layout_marginTop="1dp"
            android:layout_marginEnd="10dp"
            android:minWidth="80dp"
            android:textSize="14sp"
            android:paddingStart="3dp"
            android:paddingEnd="2dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/tvwComentario"
            app:layout_constraintTop_toBottomOf="@+id/tvwCalificadorid"
            tools:text="Fechacali" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>