<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_marginBottom="3dp"
    android:paddingStart="15dp"
    android:paddingTop="5dp"
    android:paddingEnd="15dp"
    android:paddingBottom="5dp"
    app:cardCornerRadius="4dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/tvwCnteid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:layout_marginTop="2dp"
            android:layout_marginEnd="10dp"
            android:maxWidth="50dp"
            android:minWidth="45dp"
            android:paddingStart="3dp"
            android:paddingEnd="2dp"
            android:textSize="14sp"
            app:layout_constraintEnd_toStartOf="@+id/tvwTitulo"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="Cnteid" />

        <TextView
            android:id="@+id/tvwTitulo"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:layout_marginTop="2dp"
            android:layout_marginEnd="10dp"
            android:minWidth="150dp"
            android:textSize="14sp"
            android:paddingStart="3dp"
            android:paddingEnd="2dp"
            android:textAlignment="center"
            app:layout_constraintEnd_toStartOf="@+id/tvwDescripcion"
            app:layout_constraintStart_toEndOf="@+id/tvwCnteid"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="Titulo" />

        <TextView
            android:id="@+id/tvwDescripcion"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:layout_marginTop="2dp"
            android:layout_marginEnd="10dp"
            android:minWidth="150dp"
            android:textSize="14sp"
            android:paddingStart="3dp"
            android:paddingEnd="2dp"
            android:textAlignment="center"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/tvwTitulo"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="Descripcion" />

        <TextView
            android:id="@+id/tvwPresupuesto"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="1dp"
            android:layout_marginEnd="5dp"
            android:minWidth="100dp"
            android:paddingStart="3dp"
            android:paddingEnd="2dp"
            android:textSize="14sp"
            app:layout_constraintEnd_toStartOf="@+id/tvwFechacreacion"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvwCnteid"
            tools:text="Presupuesto" />

        <TextView
            android:id="@+id/tvwFechacreacion"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:layout_marginTop="1dp"
            android:layout_marginEnd="5dp"
            android:paddingStart="3dp"
            android:paddingEnd="2dp"
            android:textSize="14sp"
            app:layout_constraintEnd_toStartOf="@+id/tvwFechalimite"
            app:layout_constraintStart_toEndOf="@+id/tvwPresupuesto"
            app:layout_constraintTop_toBottomOf="@+id/tvwCnteid"
            tools:text="Fechacrea" />

        <TextView
            android:id="@+id/tvwFechalimite"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:layout_marginTop="1dp"
            android:layout_marginEnd="5dp"
            android:minWidth="80dp"
            android:paddingStart="3dp"
            android:paddingEnd="2dp"
            android:textSize="14sp"
            app:layout_constraintEnd_toStartOf="@+id/tvwEstado"
            app:layout_constraintStart_toEndOf="@+id/tvwFechacreacion"
            app:layout_constraintTop_toBottomOf="@+id/tvwTitulo"
            tools:text="Fechalimite" />

        <TextView
            android:id="@+id/tvwEstado"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:layout_marginTop="1dp"
            android:layout_marginEnd="10dp"
            android:minWidth="80dp"
            android:paddingStart="3dp"
            android:paddingEnd="2dp"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/tvwFechalimite"
            app:layout_constraintTop_toBottomOf="@+id/tvwDescripcion"
            tools:text="Estado" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>